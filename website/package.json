{
  "name": "rspack-website",
  "private": true,
  "version": "1.0.0",
  "scripts": {
    "dev": "rspress dev",
    "build": "rspress build",
    "preview": "rspress preview",
    "sort-projects-words": "node ./sortProjectWords.js",
    "check:lint": "biome check --diagnostic-level=warn --write",
    "check:lint-ci": "biome check --diagnostic-level=warn",
    "check:format": "prettier . --write",
    "check:format-ci": "prettier . --check",
    "check:spell": "npx cspell",
    "check:case": "npx case-police docs/**/*.{md,mdx}",
    "check": "pnpm run check:lint && pnpm run check:format && pnpm run check:spell",
    "check:ci": "pnpm run check:lint-ci && pnpm run check:format-ci && pnpm run check:spell"
  },
  "license": "MIT",
  "packageManager": "pnpm@9.3.0",
  "dependencies": {
    "@rstack-dev/doc-ui": "1.5.1",
    "antd": "5.19.3",
    "axios": "^1.6.1",
    "date-fns": "^2.29.3",
    "markdown-to-jsx": "^7.2.1",
    "mermaid": "^10.9.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-intersection-observer": "^9.4.3",
    "semver": "^7.5.4",
    "tailwindcss": "^3.2.7",
    "framer-motion": "^11.3.4",
    "lottie-web": "5.12.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.0",
    "@rspress/plugin-rss": "^1.28.2",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "@types/semver": "^7.5.2",
    "case-police": "0.6.1",
    "cspell": "^8.7.0",
    "cspell-ban-words": "0.0.3",
    "prettier": "3.2.5",
    "rsbuild-plugin-google-analytics": "1.0.1",
    "rsbuild-plugin-open-graph": "1.0.1",
    "rspress": "1.28.2",
    "rspress-plugin-font-open-sans": "1.0.0",
    "rspress-plugin-sitemap": "^1.0.1",
    "typescript": "^5.0.4"
  }
}

{
  "name": "rspack-website",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "rspress build",
    "check": "pnpm run check:lint && pnpm run check:format && pnpm run check:spell",
    "check:case": "pnpx case-police@0.7 docs/**/*.{md,mdx}",
    "check:ci": "pnpm run check:lint-ci && pnpm run check:format-ci && pnpm run check:spell",
    "check:format": "prettier . --write && heading-case --write",
    "check:format-ci": "prettier . --check && heading-case",
    "check:lint": "biome check --diagnostic-level=warn --write",
    "check:lint-ci": "biome check --diagnostic-level=warn",
    "check:spell": "pnpx cspell@8",
    "dev": "rspress dev",
    "preview": "rspress preview",
    "sort-projects-words": "node ./sortProjectWords.js"
  },
  "dependencies": {
    "@rstack-dev/doc-ui": "1.10.8",
    "axios": "^1.11.0",
    "markdown-to-jsx": "^7.7.12",
    "mermaid": "^11.9.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "semver": "^7.7.2",
    "tailwindcss": "^3.4.17"
  },
  "devDependencies": {
    "@rsbuild/plugin-sass": "^1.3.3",
    "@rspress/core": "2.0.0-beta.25",
    "@rspress/plugin-algolia": "2.0.0-beta.25",
    "@rspress/plugin-llms": "2.0.0-beta.25",
    "@rspress/plugin-rss": "2.0.0-beta.25",
    "@shikijs/transformers": "^3.9.2",
    "@types/node": "^20.19.9",
    "@types/react": "^19.1.8",
    "@types/semver": "^7.7.0",
    "cspell-ban-words": "0.0.4",
    "heading-case": "^0.1.6",
    "prettier": "3.6.2",
    "rsbuild-plugin-google-analytics": "1.0.3",
    "rsbuild-plugin-open-graph": "1.1.0",
    "rspress-plugin-font-open-sans": "1.0.0",
    "@rspress/plugin-sitemap": "2.0.0-beta.25",
    "typescript": "^5.9.2"
  }
}

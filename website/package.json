    "markdown-to-jsx": "^9.5.7",
    "mermaid": "^11.12.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "semver": "^7.7.3",
    "tailwindcss": "^3.4.19"
  },
  "devDependencies": {
    "@rsbuild/plugin-sass": "^1.4.0",
    "@rspress/core": "2.0.0-rc.8",
    "@rspress/plugin-algolia": "2.0.0-rc.8",
    "@rspress/plugin-client-redirects": "2.0.0-rc.8",
    "@rspress/plugin-llms": "2.0.0-rc.8",
    "@rspress/plugin-rss": "2.0.0-rc.8",
    "@rspress/plugin-sitemap": "2.0.0-rc.8",
    "@shikijs/transformers": "^3.21.0",
    "@types/node": "^20.19.30",
    "@types/react": "^19.2.10",
    "@types/semver": "^7.7.1",
    "case-police": "~2.1.1",

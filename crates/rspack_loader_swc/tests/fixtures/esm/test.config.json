{
  "module": {
    "rules": [
      {
        "test": {
          "type": "regexp",
          "matcher": "\\.js$"
        },
        "use": [
          {
            "loader": "builtin:swc-loader",
            "options": " {\n              \"jsc\": {\n                \"target\": \"es2015\",\n                \"parser\": {\n                  \"syntax\": \"ecmascript\",\n                  \"jsx\": true,\n                  \"dynamicImport\": true,\n                  \"classProperty\": true,\n                  \"exportNamespaceFrom\": true,\n                  \"exportDefaultFrom\": true\n                }\n              }\n            }"
          }
        ]
      }
    ]
  },
  "devtool": "source-map"
}

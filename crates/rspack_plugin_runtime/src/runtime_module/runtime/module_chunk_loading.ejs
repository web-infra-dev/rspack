var installChunk = <%- basicFunction("data") %> {
    var ids = data.ids;
    var modules = data.modules;
    var runtime = data.runtime;
    // add "modules" to the modules object,
    // then flag all "ids" as loaded and fire callback
    var moduleId, chunkId, i = 0;
    for (moduleId in modules) {
        if (<%- HAS_OWN_PROPERTY %>(modules, moduleId)) {
            <%- MODULE_FACTORIES %>[moduleId] = modules[moduleId];
        }
    }
    if (runtime) runtime(<%- REQUIRE %>);
    for (; i < ids.length; i++) {
        chunkId = ids[i];
        if (<%- HAS_OWN_PROPERTY %>(installedChunks, chunkId) && installedChunks[chunkId]) {
            installedChunks[chunkId][0]();
        }
        installedChunks[ids[i]] = 0;
    }
    <%- WITH_ON_CHUNK_LOAD %>
};

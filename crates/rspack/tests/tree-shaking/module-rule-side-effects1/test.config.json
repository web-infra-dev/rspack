{
    "entry": {
        "main": {
            "import": [
                "./index.js"
            ]
        }
    },
    "builtins": {
        "define": {
            "process.env.NODE_ENV": "'development'"
        }
    },
    "debug": {
        "treeshake": true,
        "sideEffects": true
    },
    "module": {
        "rules": [
            {
                "test": {
                    "type": "regexp",
                    "matcher": "b.js$"
                },
                "sideEffect": false
            }
        ]
    }
}
{
  "compilerOptions": {
    "baseUrl": ".",
    "rootDir": ".",
    "target": "ESNext",
    "module": "ESNext",
    "noEmit":true,
    "esModuleInterop": true,
    "resolveJsonModule":true,
    "moduleResolution": "nodenext",
    "allowImportingTsExtensions": true,
    "allowSyntheticDefaultImports": true,
  },
  "include": ["**/*.ts", "**/*.js"]
}

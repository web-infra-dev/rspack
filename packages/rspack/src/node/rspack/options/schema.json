{
  "$schema": "http://json-schema.org/schema",
  "type": "object",
  "additionalProperties": false,
  "properties": {
    "chunkFilename": {
      "type": "string"
    },
    "mode": {
      "type": "string",
      "enum": [
        "production",
        "development",
        "none"
      ]
    },
    "entries": {
      "type": "object",
      "additionalProperties": false,
      "patternProperties": {
        ".*": {
          "type": "string"
        }
      },
      "minProperties": 1
    },
    "loader": {
      "type": "object",
      "additionalProperties": false,
      "patternProperties": {
        ".*": {
          "type": "string",
          "enum": [
            "css",
            "less",
            "sass",
            "dataURI",
            "js",
            "jsx",
            "ts",
            "tsx",
            "null",
            "json",
            "text"
          ]
        }
      }
    },
    "resolve": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "alias": {
          "type": "object",
          "additionalProperties": false,
          "patternProperties": {
            ".*": {
              "type": "string"
            }
          }
        },
        "conditionNames": {
          "type": "array",
          "items": {
            "type": "string"
          }
        },
        "aliasFields": {
          "type": "string"
        }
      }
    },
    "output": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "outdir": {
          "type": "string"
        },
        "entryFilename": {
          "type": "string"
        },
        "sourceMap": {
          "oneOf": [
            {
              "enum": [
                "inline",
                "external",
                "linked"
              ]
            },
            {
              "type": "boolean"
            }
          ]
        }
      }
    },
    "optimization": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "minify": {
          "type": "boolean"
        },
        "splitChunks": {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "codeSplitting": {
              "type": "boolean"
            },
            "reuseExstingChunk": {
              "type": "boolean"
            }
          }
        },
        "removeEmptyChunks": {
          "type": "boolean"
        },
        "chunkIdAlgo": {
          "type": "string",
          "enum": [
            "named"
          ]
        },
        "moduleIdAlgo": {
          "type": "string",
          "enum": [
            "named"
          ]
        }
      }
    },
    "enhanced": {
      "type": "object",
      "additionalProperties": false,
      "properties": {
        "globals": {
          "type": "object",
          "additionalProperties": false,
          "patternProperties": {
            ".*": {
              "type": "string"
            }
          }
        },
        "svgr": {
          "type": "boolean"
        },
        "progress": {
          "type": "boolean"
        },
        "lazyCompilation": {
          "type": "boolean"
        },
        "inlineStyle": {
          "type": "boolean"
        },
        "react": {
          "type": "object",
          "additionalProperties": false,
          "properties": {
            "fastRefresh": {
              "type": "boolean"
            }
          }
        }
      }
    },
    "root": {
      "type": "string"
    }
  },
  "required": [
    "entries"
  ]
}
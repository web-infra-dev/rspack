{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "definitions": {
        "RawEnhancedOptions": {
            "properties": {
                "define": {
                    "$ref": "#/definitions/Record<string,string>"
                },
                "globals": {
                    "$ref": "#/definitions/Record<string,string>"
                },
                "inlineStyle": {
                    "type": "boolean"
                },
                "lazyCompilation": {
                    "type": "boolean"
                },
                "progress": {
                    "type": "boolean"
                },
                "react": {
                    "$ref": "#/definitions/RawReactOptions"
                },
                "svgr": {
                    "type": "boolean"
                }
            },
            "type": "object"
        },
        "RawOptimizationOptions": {
            "properties": {
                "chunkIdAlgo": {
                    "type": "string"
                },
                "minify": {
                    "type": "boolean"
                },
                "moduleIdAlgo": {
                    "type": "string"
                },
                "removeEmptyChunks": {
                    "type": "boolean"
                },
                "splitChunks": {
                    "$ref": "#/definitions/RawSplitChunksOptions"
                }
            },
            "type": "object"
        },
        "RawOutputOptions": {
            "properties": {
                "entryFilename": {
                    "type": "string"
                },
                "outdir": {
                    "type": "string"
                },
                "sourceMap": {
                    "enum": [
                        "external",
                        "inline",
                        "linked",
                        "none"
                    ],
                    "type": "string"
                }
            },
            "type": "object"
        },
        "RawReactOptions": {
            "properties": {
                "fastRefresh": {
                    "type": "boolean"
                }
            },
            "type": "object"
        },
        "RawResolveOptions": {
            "properties": {
                "alias": {
                    "$ref": "#/definitions/Record<string,string>"
                },
                "aliasField": {
                    "type": "string"
                },
                "conditionNames": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                }
            },
            "type": "object"
        },
        "RawSplitChunksOptions": {
            "properties": {
                "codeSplitting": {
                    "type": "boolean"
                },
                "reuseExstingChunk": {
                    "type": "boolean"
                }
            },
            "type": "object"
        },
        "Record<string,string>": {
            "type": "object"
        }
    },
    "properties": {
        "chunkFilename": {
            "type": "string"
        },
        "enhanced": {
            "$ref": "#/definitions/RawEnhancedOptions"
        },
        "entries": {
            "$ref": "#/definitions/Record<string,string>"
        },
        "loader": {
            "$ref": "#/definitions/Record<string,string>"
        },
        "mode": {
            "enum": [
                "development",
                "none",
                "production"
            ],
            "type": "string"
        },
        "optimization": {
            "$ref": "#/definitions/RawOptimizationOptions"
        },
        "output": {
            "$ref": "#/definitions/RawOutputOptions"
        },
        "resolve": {
            "$ref": "#/definitions/RawResolveOptions"
        },
        "root": {
            "type": "string"
        }
    },
    "required": [
        "entries"
    ],
    "type": "object"
}


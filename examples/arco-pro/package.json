{
  "name": "example-arco-design-pro",
  "version": "1.0.0",
  "description": "Arco Design Pro",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development ./node_modules/@rspack/cli/bin/rspack serve -c rspack.config.js",
    "build": "NODE_ENV=production ./node_modules/@rspack/cli/bin/rspack build -c rspack.config.js",
    "analyze": "pnpm run build --analyze",
    "preview": "cd dist && serve -s",
    "bundle-stats": "NODE_ENV=production ./node_modules/@rspack/cli/bin/rspack build -c rspack.config.js --json ./rspack-stats.json",
    "webpack:dev": "NODE_ENV=development webpack serve -c webpack.config.js",
    "webpack:build": "NODE_ENV=production webpack build -c webpack.config.js",
    "webpack:bundle-stats": "NODE_ENV=production webpack build -c webpack.config.js --json ./webpack-stats.json",
    "compare-bundle-stats": "pnpm run bundle-stats && pnpm run webpack:bundle-stats && bundle-stats ./rspack-stats.json ./webpack-stats.json"
  },
  "dependencies": {
    "@antv/data-set": "^0.11.8",
    "@arco-design/color": "^0.4.0",
    "@arco-design/web-react": "2.29.2",
    "@arco-themes/react-arco-pro": "^0.0.7",
    "@loadable/component": "^5.15.2",
    "@turf/turf": "^6.5.0",
    "arco-design-pro": "^2.3.0",
    "axios": "^0.24.0",
    "bizcharts": "^4.1.15",
    "classnames": "^2.3.1",
    "copy-to-clipboard": "^3.3.1",
    "dayjs": "^1.10.7",
    "lodash": "^4.17.21",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "query-string": "^6.14.1",
    "react": "^17.0.2",
    "react-color": "^2.19.3",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router": "^5.2.1",
    "react-router-dom": "^5.3.0",
    "redux": "^4.1.2",
    "regenerator-runtime": "0.13.9"
  },
  "devDependencies": {
    "serve": "14.1.2",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.9",
    "@rspack/cli": "workspace:*",
    "@rspack/plugin-html": "workspace:*",
    "@svgr/core": "6.5.1",
    "@svgr/webpack": "^6.5.1",
    "@swc/core": "^1.3.14",
    "bundle-stats": "^4.1.2",
    "css-loader": "^6.7.1",
    "html-webpack-plugin": "^5.5.0",
    "less-loader": "^11.1.0",
    "postcss-loader": "7.0.2",
    "mini-css-extract-plugin": "^2.6.1",
    "style-loader": "^3.3.1",
    "swc-loader": "^0.2.3",
    "typescript": "^4.5.5",
    "webpack": "5.74.0",
    "webpack-cli": "4.10.0"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "rspack": {}
}